{
  "expo": {
    "name": "vibecode",
    "slug": "vibecode",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "plugins": ["expo-router", ["expo-font", { "fonts": ["./assets/fonts/sf-pro-rounded-semibold.ttf"] }]],
    "experiments": {
      "typedRoutes": true
    }
  }
}
